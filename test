function doautosell()
spawn(function()
    while autosell do
        pcall(function()
            local teleportbackaftersell = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(532.799988, 185.8358, 151.485992, 1, 1.79191293e-08, -1.57503391e-13, -1.79191293e-08, 1, -5.27863406e-08, 1.56557506e-13, 5.27863406e-08, 1)
            task.wait(0.2)
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(532.799988, 185.8358, 151.485992, 1, 1.79191293e-08, -1.57503391e-13, -1.79191293e-08, 1, -5.27863406e-08, 1.56557506e-13, 5.27863406e-08, 1)
            task.wait(0.2)
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = teleportbackaftersell
        end)
        task.wait(waitingtimeforsell)
        end
    end)
end


function buyeggs()
    spawn(function()
        while getgenv().autoegg do
            local hwid =
                {
                    game:GetService("ReplicatedStorage").Eggs[selectedEgg],
                    3
                }
            game:GetService("ReplicatedStorage").Events.HatchEggs:InvokeServer(unpack(hwid))
            task.wait()
        end
    end)
end


function BringBossFarm()
    spawn(function()
        while getgenv().BringBossKiller do
            if  game.Workspace:FindFirstChild("Boss") then
                for N,M in pairs(game.Workspace:FindFirstChild("Boss"):GetDescendants())do 
                    if  M.Name=="HumanoidRootPart"then 
                        task.wait()
                        M.Anchored=true;
                        M.CFrame=game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame;
                        local RunService = game:GetService("RunService")
                        local MaxFPS = 4
                        local t0 = tick()
                        RunService.Heartbeat:Wait()
                        repeat until (t0 + 1/MaxFPS) < tick()
                    end
                end
            end
            task.wait()
        end
    end)
end

function bringelements()
    spawn(function()
        while getgenv().BringElements do
            if  game:GetService("Workspace").Mobs:GetChildren() then
            for N,M in pairs(game.Workspace.Mobs:GetDescendants())do
                    if  M.Name=="HumanoidRootPart"then
                        task.wait()
                        M.Anchored = true
                        M.CFrame=game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame;
                        local RunService = game:GetService("RunService")
                        local MaxFPS = 7
                        local t0 = tick()
                        RunService.Heartbeat:Wait()
                        repeat until (t0 + 1/MaxFPS) < tick()
                    end
                end
            task.wait()
            end
        end
    end)
end


function autocombine()
    spawn(function()
        while getgenv().AutoCombine do
            game:GetService("ReplicatedStorage").Events.CombineAll:FireServer()
            task.wait()
        end
    end)
end

function BossFarm()
    spawn(function()
        while getgenv().BossKiller do
            local plr = game.Players.LocalPlayer
            task.wait()
            pcall(function()
                local Boss = game:GetService("Workspace").Boss.HumanoidRootPart.Position
                local boss = game.Workspace:FindFirstChild("Boss")
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Boss.X, 186.29756164551, Boss.Z) 
            end)
        end
        task.wait()
    end)
end

function DailyReward()
    spawn(function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(workspace.DailyReward.Location.CFrame.Position + Vector3.new(0,4,0))
    end)
end

