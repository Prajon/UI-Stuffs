function removetexture()
    spawn(function()
        for _,v in pairs(workspace:GetDescendants()) do
            if v.ClassName == "Part"
                or v.ClassName == "SpawnLocation"
                or v.ClassName == "WedgePart"
                or v.ClassName == "Terrain"
                or v.ClassName == "MeshPart" then
                v.BrickColor = BrickColor.new(155, 155, 155)
                v.Material = "Plastic"
            end
        end
    end)
end


function autocandy()
    spawn(function()
        game.Players.LocalPlayer.PlayerGui.Gui.Home.MSG.Text = "This shit is fixed :)"
        task.wait(2)
        game.Players.LocalPlayer.PlayerGui.Gui.Home.MSG.Text = ""
    end)
    
    
    spawn(function()
        while getgenv().candy do
            pcall(function()
            for i,v in pairs(game:GetService("Workspace").CandyHolder:GetDescendants()) do
                if v.Name == "CandyCane" or v.Name == "Present"  then
                if v.Transparency == 0 then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                else
                end
                end
                end
                end)
            task.wait(0.5)
        end
    end)
end

else	
game.Players.LocalPlayer.PlayerGui.Gui.Home.MSG.Text = "Please join \n www.roblox.com/groups/8939440/SenpaiChou to verify yourself \n if you want any help join discord.gg/FhzbB7hXGr"
--game:service('Players').LocalPlayer:Kick('Please join www.roblox.com/groups/8939440/SenpaiChou to verify yourself or if you want any help join discord.gg/FhzbB7hXGr')
end
