    -- Scripts:

    local function RVCATHU_fake_script() -- MainFrame.Draggable 
        local script = Instance.new('LocalScript', MainFrame)

        local UserInputService = game:GetService("UserInputService")
        
        local gui = script.Parent
        
        local dragging
        local dragInput
        local dragStart
        local startPos
        
        local function update(input)
            local delta = input.Position - dragStart
            gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
        
        gui.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                dragging = true
                dragStart = input.Position
                startPos = gui.Position
        
                input.Changed:Connect(function()
                    if input.UserInputState == Enum.UserInputState.End then
                        dragging = false
                    end
                end)
            end
        end)
        
        gui.InputChanged:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
                dragInput = input
            end
        end)
        
        UserInputService.InputChanged:Connect(function(input)
            if input == dragInput and dragging then
                update(input)
            end
        end)
    end
    coroutine.wrap(RVCATHU_fake_script)()
    local function REQMOH_fake_script() -- CloseButton.CloseScript 
        local script = Instance.new('LocalScript', CloseButton)

        script.Parent.MouseEnter:Connect(function()
            script.Parent.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
        end)
        
        script.Parent.MouseLeave:Connect(function()
            script.Parent.BackgroundColor3 = Color3.fromRGB(32, 32, 32)
        end)
        
        local function onInputBegan(input, gameProcessed)
            if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                script.Parent.Parent.Parent:Destroy()
            end
        end
        script.Parent.InputBegan:Connect(onInputBegan)
    end
    coroutine.wrap(REQMOH_fake_script)()
    local function YXHYPPI_fake_script() -- DiscordButton.DiscordScript 
        local script = Instance.new('LocalScript', DiscordButton)

        script.Parent.MouseEnter:Connect(function()
            script.Parent.TextColor3 = Color3.fromRGB(155, 155, 155)
        end)
        
        script.Parent.MouseLeave:Connect(function()
            script.Parent.TextColor3 = Color3.fromRGB(255, 255, 255)
        end)
        
        local function onInputBegan(input, gameProcessed)
            if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                loadstring(game:HttpGet("https://raw.githubusercontent.com/myladoisbig/DEVIL-Hub/main/discord-link"))()
                script.Parent.Text = "Discord invite copied"
                task.wait(1)
                script.Parent.Text = "Copy discord invite"
            end
        end
        script.Parent.InputBegan:Connect(onInputBegan)
    end
    coroutine.wrap(YXHYPPI_fake_script)()
    local function URVMO_fake_script() -- EnterButton.KeyScript 
        local script = Instance.new('LocalScript', EnterButton)

        local KeyCheck = script.Parent.Parent.KeyInsertBox
        local SupportedCheck = script.Parent.Parent.TitleLabel
        
        script.Parent.MouseEnter:Connect(function()
            script.Parent.TextColor3 = Color3.fromRGB(155, 155, 155)
        end)
        
        script.Parent.MouseLeave:Connect(function()
            script.Parent.TextColor3 = Color3.fromRGB(255, 255, 255)
        end)
        local function onInputBegan(input, gameProcessed)
            if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                if SupportedCheck.Text == "DEVIL Hub | Supported" then
                    task.wait(0.5)
                    KeyCheck.Text = "Loading..."
                    task.wait(1)
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/myladoisbig/DEVIL-Hub/main/games"))()
                    script.Parent.Parent.Parent:Destroy()
                else
                    if SupportedCheck.Text == "DEVIL Hub | Not Supported" then
                        task.wait(0.5)
                        KeyCheck.Text = "Error!!! - Not Supported Game"
                        task.wait(1)
                        KeyCheck.Text = "Premium..."
                    end
                end
            end
        end
        script.Parent.InputBegan:Connect(onInputBegan)
    end
    coroutine.wrap(URVMO_fake_script)()
    local function YOVY_fake_script() -- KeyButton.KeyScript 
        local script = Instance.new('LocalScript', KeyButton)

        script.Parent.MouseEnter:Connect(function()
            script.Parent.TextColor3 = Color3.fromRGB(155, 155, 155)
        end)
        
        script.Parent.MouseLeave:Connect(function()
            script.Parent.TextColor3 = Color3.fromRGB(255, 255, 255)
        end)
        
        script.Parent.MouseButton1Click:Connect(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/myladoisbig/DEVIL-Hub/main/Key"))()
            script.Parent.Text = "Key address copied"
            task.wait(1)
            script.Parent.Text = "Copy key address"
        end)
    end
    coroutine.wrap(YOVY_fake_script)()
    local function IRFJDSA_fake_script() -- AvatarPreview.AvatarScript 
        local script = Instance.new('LocalScript', AvatarPreview)

        script.Parent.Image = game:GetService("Players"):GetUserThumbnailAsync(game.Players.LocalPlayer.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
    end
    coroutine.wrap(IRFJDSA_fake_script)()
    local function PHUKWZV_fake_script() -- PlayerName.PlayerNameScript 
        local script = Instance.new('LocalScript', PlayerName)

        script.Parent.Text = game.Players.LocalPlayer.Name .." (" ..game.Players.LocalPlayer.DisplayName ..")"
    end
    coroutine.wrap(PHUKWZV_fake_script)()
    local function ZDCJHD_fake_script() -- TitleLabel.TitleLabelScript 
        local script = Instance.new('LocalScript', TitleLabel)

        local supportedgame = loadstring(game:HttpGet("https://raw.githubusercontent.com/myladoisbig/DEVIL-Hub/main/Supported-Game-Checker"))()
        local GameId = game.PlaceId
        if supportedgame[GameId] then
            wait(0.5)
            script.Parent.Text = "DEVIL Hub | Supported"
        else
            wait(0.5)
            script.Parent.Text = "DEVIL Hub | Not Supported"
        end
    end
    coroutine.wrap(ZDCJHD_fake_script)()
