                        if v.ClassName == "ImageLabel" then
                            if v.Name == "Safe"  then
                                if v.Visible == false then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Parent.Parent.Parent.Parent.HumanoidRootPart.CFrame
                                end
                            end
                        end
                    end
                end)
            task.wait()
            end
        end)
