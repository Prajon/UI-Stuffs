local DEVIL_UI_Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Prajon/UI-Stuffs/main/DEVIL-Ui-Lib", true))()

local Window = DEVIL_UI_Library:CreateWindow("DEVIL Script", game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name)

local tab1 = Window:CreateTab("Main")


tab1:CreateButton("Disable Error(Not Sound)", function()
	for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.UI:GetDescendants()) do
        if v.Name == "ErrorFrame" then
            v:Remove()
        end
    end
end)


tab1:CreateButton("Disable(Purchase Product)", function()
	for i,v in pairs(game.CoreGui:GetChildren()) do
        if v.Name == "PurchasePrompt" then
            v:Remove()
        end
    end
end)


getgenv().autoclick = false
tab1:CreateToggle("Auto Click", function(bool)
	getgenv().autoclick = bool
    if bool then
        spawn(function()
            while autoclick do wait()
                pcall(function()
                    game:GetService("ReplicatedStorage").Remotes.Tapping:FireServer("Tapping")
                end)
            end
        end)
    end
end)


getgenv().autoTraining = false
tab1:CreateToggle("Auto Training", function(bool)
	getgenv().autoTraining = bool
    if bool then
        spawn(function()
            while autoTraining do wait()
                pcall(function()
                    local lphrp = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart
                    lphrp.CFrame = CFrame.new(-48.0543175, -18.4973812, -37.576355, 0.256209582, 5.10446476e-08, -0.96662128, 3.15116431e-08, 1, 6.11596676e-08, 0.96662128, -4.6129518e-08, 0.256209582)
                    local args = {
                        [1] = "Training",
                        [2] = workspace.__WORKSPACE.__Interact.Training
                    }
                    
                    game:GetService("ReplicatedStorage").Remotes.Training:FireServer(unpack(args))
                end)
            end
        end)
    end
end)


local condomrebirth = {}
for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.UI.CenterFrame.Rebirths.Frame:GetDescendants()) do
    if v.Name == "RebirthUID" then
        table.insert(condomrebirth, v.Text)
    end
end

local selectedrebirth
tab1:CreateDropdown('Select Rebirth', condomrebirth, function(value)
    selectedrebirth = value
end)


getgenv().autorebirth = false
tab1:CreateToggle("Auto Rebirth", function(bool)
	getgenv().autorebirth = bool
    if bool then
        spawn(function()
            while autorebirth do wait()
                pcall(function()
                    for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.UI.CenterFrame.Rebirths.Frame:GetDescendants()) do
                        if v.Name == "RebirthUID" then
                            if v.Text == selectedrebirth then
                                local args = {
                                [1] = "Rebirths",
                                [2] = tonumber(v.Parent.Parent.Name)
                                }
                                
                                game:GetService("ReplicatedStorage").Remotes.RefeshClient:FireServer(unpack(args))
                            end
                        end
                    end
                end)
            end
        end)
    end
end)


local condomeggs = {}
for i,v in pairs(game:GetService("Workspace")["__WORKSPACE"]["__Eggs"]:GetChildren()) do
    table.insert(condomeggs, v.Name)
end

local selectedegg
tab1:CreateDropdown('Select Egg', condomeggs, function(value)
    selectedegg = value
end)

getgenv().autoegg = false
tab1:CreateToggle("Auto Egg", function(bool)
	getgenv().autoegg = bool
    if bool then
        spawn(function()
            while autoegg do wait()
                pcall(function()
                    for i,v in pairs(game:GetService("Workspace")["__WORKSPACE"]["__Eggs"]:GetChildren()) do
                        if v.Name == selectedegg then
                            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.CF.CFrame
                            local args = {
                                [1] = "EGG",
                                [2] = {
                                    [1] = workspace.__WORKSPACE.__Eggs:FindFirstChild(selectedegg),
                                    [2] = 1,
                                    [3] = {}
                                }
                            }
                            
                            game:GetService("ReplicatedStorage").Remotes.RefeshClient:FireServer(unpack(args))
                        end
                    end
                end)
            end
        end)
    end
end)


getgenv().autocollectdaily = false
tab1:CreateToggle("Auto Collect Daily", function(bool)
	getgenv().autocollectdaily = bool
    if bool then
        spawn(function()
            while autocollectdaily do wait(5)
                pcall(function()
                    local args = {
                        [1] = "Daily"
                    }
                    
                    game:GetService("ReplicatedStorage").Remotes.RefeshClient:FireServer(unpack(args))
                end)
                wait(5)
            end
        end)
    end
end)


getgenv().autobuyranks = false
tab1:CreateToggle("Auto Buy Ranks", function(bool)
	getgenv().autobuyranks = bool
    if bool then
        spawn(function()
            while autobuyranks do wait(5)
                pcall(function()
                    for i = 1, 50 do
                        local args = {
                            [1] = "Ranks",
                            [2] = i
                        }
                        
                        game:GetService("ReplicatedStorage").Remotes.RefeshClient:FireServer(unpack(args))
                    end
                end)
                wait(5)
            end
        end)
    end
end)


getgenv().autobuytrainer = false
tab1:CreateToggle("Auto Buy Trainer", function(bool)
	getgenv().autobuytrainer = bool
    if bool then
        spawn(function()
            while autobuytrainer do wait(5)
                pcall(function()
                    local args = {
                        [1] = "Trainer",
                        [2] = "Normal"
                    }
                    
                    game:GetService("ReplicatedStorage").Remotes.RefeshClient:FireServer(unpack(args))
                end)
                wait(5)
            end
        end)
    end
end)


getgenv().autobuyareas = false
tab1:CreateToggle("Auto Buy Areas", function(bool)
	getgenv().autobuyareas = bool
    if bool then
        spawn(function()
            while autobuyareas do wait(5)
                pcall(function()
                    for i = 1, 50 do
                        local args = {
                            [1] = "Areas",
                            [2] = i
                        }
                    
                        game:GetService("ReplicatedStorage").Remotes.ClientRefeshFunction:InvokeServer(unpack(args))
                    end
                end)
                wait(5)
            end
        end)
    end
end)


getgenv().autoclaimgifts = false
tab1:CreateToggle("Auto Claim Gifts", function(bool)
	getgenv().autoclaimgifts = bool
    if bool then
        spawn(function()
            while autoclaimgifts do wait(5)
                pcall(function()
                    for i = 1, 50 do
                        local args = {
                            [1] = "Gifts",
                            [2] = "Gift"..i
                        }
                        
                        game:GetService("ReplicatedStorage").Remotes.RefeshClient:FireServer(unpack(args))
                    end                    
                end)
                wait(5)
            end
        end)
    end
end)
