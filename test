local AllElements = 
{
    "Fire",
    "Water",
    "Earth",
    "Plasma"
}
local SelectedElement;
xx:Dropdown("Select Element",(AllElements),true,function(thiselement)
    SelectedElement = thiselement;
    print(thiselement)
end)


xx:Toggle("Farm Element",function(bool)
    getgenv().autoelementfarm = bool
    if bool then
        spawn(function()
            while autoelementfarm do
                pcall(function()
                    function getNear()
                    local near  
                    local nearr = math.huge
                        for i,v in pairs(game:GetService("Workspace").Mobs:GetChildren()) do
                            if v.Name:find(SelectedElement) then
                                for a,b in pairs(v:GetChildren()) do
                                    if b.Name == "HumanoidRootPart" then
                                        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - b.Position).Magnitude < nearr then
                                            near = b
                                            nearr = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - b.Position).Magnitude
                                        end
                                    end
                                end
                            end
                        end
                        return near
                    end
                    local nearest = getNear()
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = nearest.CFrame
                end)
                task.wait()
            end
        end)
    end
end)

