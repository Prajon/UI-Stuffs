        local function MIOT_fake_script() -- EnterButton.KeyScript 
            local script = Instance.new('LocalScript', EnterButton)

            local KeyCheck = script.Parent.Parent.KeyInsertBox
            local SupportedCheck = script.Parent.Parent.TitleLabel
            local KeyLink = loadstring(game:HttpGet("https://raw.githubusercontent.com/myladoisbig/DEVIL-Hub/main/kestam"))()
            local OldKey = loadstring(game:HttpGet("https://pastebin.com/raw/aQXH81Bf"))()
            
            script.Parent.MouseEnter:Connect(function()
                script.Parent.TextColor3 = Color3.fromRGB(155, 155, 155)
            end)
            
            script.Parent.MouseLeave:Connect(function()
                script.Parent.TextColor3 = Color3.fromRGB(255, 255, 255)
            end)
            task.wait(0.5)
            
            local function onInputBegan(input, gameProcessed)
                if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                    if KeyLink[KeyCheck.Text] and SupportedCheck.Text == "DEVIL Hub | Supported" then
                        task.wait(0.5)
                        KeyCheck.Text = "Correct Key"
                        task.wait(1)
                        KeyCheck.Text = "Loading..."
                        task.wait(1)
                        loadstring(game:HttpGet("https://raw.githubusercontent.com/myladoisbig/DEVIL-Hub/main/games"))()
                        script.Parent.Parent.Parent:Destroy()
                    else
                        if OldKey[KeyCheck.Text] then
                            task.wait(0.5)
                            KeyCheck.Text = "Outdated Key"
                            task.wait(1)
                            KeyCheck.Text = "Insert Key..."
                        else
                            if not KeyLink[KeyCheck.Text] and not OldKey[KeyCheck.Text] then
                                task.wait(0.5)
                                KeyCheck.Text = "Incorrect Key"
                                task.wait(1)
                                KeyCheck.Text = "Insert Key..."
                            else
                                if KeyLink[KeyCheck.Text] and SupportedCheck.Text == "DEVIL Hub | Not Supported" then
                                    task.wait(0.5)
                                    KeyCheck.Text = "Correct Key"
                                    task.wait(1)
                                    KeyCheck.Text = "Error!!! - Not Supported Game"
                                    task.wait(1)
                                    KeyCheck.Text = "Insert Key..."
                                end
                            end
                        end
                    end
                end
            end
            script.Parent.InputBegan:Connect(onInputBegan)
            
        end
        coroutine.wrap(MIOT_fake_script)()
        local function XJMTIKI_fake_script() -- KeyButton.KeyScript 
            local script = Instance.new('LocalScript', KeyButton)

            script.Parent.MouseEnter:Connect(function()
                script.Parent.TextColor3 = Color3.fromRGB(155, 155, 155)
            end)
            
            script.Parent.MouseLeave:Connect(function()
                script.Parent.TextColor3 = Color3.fromRGB(255, 255, 255)
            end)
            
            script.Parent.MouseButton1Click:Connect(function()
                loadstring(game:HttpGet("https://raw.githubusercontent.com/myladoisbig/DEVIL-Hub/main/Key"))()
                script.Parent.Text = "Key address copied"
                task.wait(1)
                script.Parent.Text = "Copy key address"
            end)
        end
        coroutine.wrap(XJMTIKI_fake_script)()
        local function BFYEHY_fake_script() -- AvatarPreview.AvatarScript 
            local script = Instance.new('LocalScript', AvatarPreview)

            script.Parent.Image = game:GetService("Players"):GetUserThumbnailAsync(game.Players.LocalPlayer.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
        end
        coroutine.wrap(BFYEHY_fake_script)()
        local function REBQ_fake_script() -- PlayerName.PlayerNameScript 
            local script = Instance.new('LocalScript', PlayerName)

            script.Parent.Text = game.Players.LocalPlayer.Name .." (" ..game.Players.LocalPlayer.DisplayName ..")"
        end
        coroutine.wrap(REBQ_fake_script)()
        local function MCFKNZ_fake_script() -- TitleLabel.TitleLabelScript 
            local script = Instance.new('LocalScript', TitleLabel)

            local supportedgame = loadstring(game:HttpGet("https://raw.githubusercontent.com/myladoisbig/DEVIL-Hub/main/Supported-Game-Checker"))()
            local GameId = game.PlaceId
            if supportedgame[GameId] then
                wait(0.5)
                script.Parent.Text = "DEVIL Hub | Supported"
            else
                wait(0.5)
                script.Parent.Text = "DEVIL Hub | Not Supported"
            end
        end
        coroutine.wrap(MCFKNZ_fake_script)()
        
end
        
end
